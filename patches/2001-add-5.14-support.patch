++	int ret;
++
++	ret = xt_flowoffload_route_dir(route, ct, dir, par,
++				       dev[dir]->ifindex);
++	if (ret)
++		return ret;
++
++	ret = xt_flowoffload_route_dir(route, ct, !dir, par,
++				       dev[!dir]->ifindex);
++	if (ret)
++		return ret;
++
++	xt_flowoffload_route_check_path(route, ct, dir, &dev[!dir]);
++	xt_flowoffload_route_check_path(route, ct, !dir, &dev[dir]);
++
++	return 0;
++}
